<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="SeleniumScreenshots">
  <parameter name="selenium.browser" value="*googlechrome" />
  <parameter name="selenium.url" value="http://localhost/" />
  <parameter name="screenshoter.properties" value="/screenshoter.properties" />
  <listeners>
    <listener class-name="ru.st.selenium.ScreenshotListener"></listener>
  </listeners>
  <test verbose="2" name="ru.st.samples.ScreenshotSample" annotations="JDK">
    <classes>
      <class name="ru.st.samples.ScreenshotSample"></class>
    </classes>
  </test>
</suite>
